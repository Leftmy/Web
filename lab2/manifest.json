{
  "manifest_version": 3,
  "name": "Currency Converter",
  "version": "1.0",
  "description": "Convert currencies easily.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["storage", "activeTab"]
}
